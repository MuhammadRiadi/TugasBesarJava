    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
    
    g.setSteker(Integer.valueOf(stopkontak.getText()));
    g.setLcd(Integer.valueOf(lcd.getText()));
    g.setLampu(Integer.valueOf(lampu.getText()));
    g.setKipas(Integer.valueOf(kipas.getText()));
    g.setAc(Integer.valueOf(ac.getText()));
    g.setInternet(String.valueOf(internet.getText()));
    g.setLogin(String.valueOf(kinternet.getText()));
    g.setKondisiac(String.valueOf(kac.getText()));
    g.setKondisisteker(String.valueOf(ksteker.getText()));
    g.setKondisikipas(String.valueOf(kipas.getText()));
    g.setKondisicctv(String.valueOf(kcctv.getText()));
    g.setKondisilcd(String.valueOf(klcd.getText()));
    g.setKondisilampu(String.valueOf(klampu.getText()));
    g.setPosisisteker(String.valueOf(psteker.getText()));
    g.setPosisilcd(String.valueOf(plcd.getText()));
    g.setPosisikipas(String.valueOf(pkipas.getText()));
    g.setPosisilampu(String.valueOf(plampu.getText()));
    g.setPosisiac(String.valueOf(pac.getText()));
    g.setPosisicctv(String.valueOf(pcctv.getText()));
    
    }                                        

