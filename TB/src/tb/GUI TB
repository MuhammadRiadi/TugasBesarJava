KondisiRuangKelas b=new KondisiRuangKelas(1,1, 1, 1, 1, 1,1 );
    private void ruangkelasActionPerformed(java.awt.event.ActionEvent evt) {                                           

b.setPanjang(Integer.valueOf(panjang.getText()));
b.setLebar(Integer.valueOf(lebar.getText()));
b.setJumlahkursi(Integer.valueOf(kursi.getText()));
b.setPintu(Integer.valueOf(pintu.getText()));
b.setJendela(Integer.valueOf(jendela.getText()));

luas.setText(String.valueOf(Integer.valueOf(b.getLuas())));
rasio.setText(String.valueOf(b.getRasio()));
pintujendela.setText(String.valueOf(b.getPintuJendela()));
bentuk.setText(String.valueOf(b.Bentuk()));


    }                                          
 KeamananRuangKelas c=new KeamananRuangKelas(null, null, null);
    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
     
     c.setKekokohan(String.valueOf(KOKOH.getText()));
     c.setKunci(String.valueOf(ADA.getText()));
     c.setKeamanan(String.valueOf(AMAN.getText()));
     kekokohan.setText(String.valueOf(c.getKekokohan()));
     kunci.setText(String.valueOf(c.getKunci()));
     keamanan.setText(String.valueOf(c.getKeamanan()));
     
     
    }                                        

