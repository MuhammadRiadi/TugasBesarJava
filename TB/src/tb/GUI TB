KebersihanRuangKelas d=new KebersihanRuangKelas(null, 1, 1, 1);
    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        
        d.setUdara(String.valueOf(udara.getText()));
        d.setKelembapan(Integer.valueOf(kelembapan.getText()));
        d.setCahaya(Integer.valueOf(cahaya.getText()));
        d.setSuhu(Integer.valueOf(suhu.getText()));
        
        udara1.setText(String.valueOf(d.getUdara()));
        kelembapan1.setText(String.valueOf(Integer.parseInt(d.getKelembapan())));
        cahaya1.setText(String.valueOf(Integer.parseInt(d.getCahaya())));
        suhu1.setText(String.valueOf(Integer.valueOf(d.getSuhu())));
        
       
    }                                        
KenyamananRuangKelas e=new KenyamananRuangKelas(null, null, null, null, null)  ;
    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         

e.setBising(String.valueOf(bising.getText()));
e.setKeausan(String.valueOf(aus.getText()));
e.setBau(String.valueOf(bau.getText()));
e.setKebocoran(String.valueOf(bocor.getText()));
e.setKerusakan(String.valueOf(rusak.getText()));
bising1.setText(String.valueOf(e.getBau()));
aus1.setText(String.valueOf(e.getKeausan()));
bau1.setText(String.valueOf(e.getBau()));
bocor1.setText(String.valueOf(e.getKebocoran()));
